{
  "publisher": "KalimahApps",
  "name": "tabaqa",
  "displayName": "tabaqa",
  "description": "Extendable and nested settings for VSCode",
  "version": "0.0.8",
  "repository": {
    "type": "git",
    "url": "https://github.com/kalimahapps/tabaqa.git"
  },
  "bugs": {
    "url": "https://github.com/kalimahapps/tabaqa/issues"
  },
  "license": "MIT",
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "jsonValidation": [
      {
        "fileMatch": [
          "tabaqa.json"
        ],
        "url": "https://raw.githubusercontent.com/kalimahapps/tabaqa/main/schema.json"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p tsconfig.json --listEmittedFiles && tsc-alias -p tsconfig.json",
    "coverage": "c8 --clean npm run test",
    "pretest": "npm run compile",
    "test": "npm run compile && node ./dist/test/run-test.js",
    "vscode:prepublish": "npm run compile",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@kalimahapps/eslint-config": "^1.1.7",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.14.2",
    "@types/vscode": "^1.90.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vscode/test-electron": "^2.4.0",
    "c8": "^7.12.0",
    "c8js": "^0.5.0",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.4.0",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.1.6"
  },
  "galleryBanner": {
    "color": "#2c2c32",
    "theme": "dark"
  },
  "keywords": [
    "settings",
    "nested",
    "extendable",
    "vscode",
    "vscode-extension",
    "vscode-settings",
    "vscode-settings-extension"
  ],
  "preview": true,
  "categories": [
    "Other"
  ],
  "main": "./dist/extension.js",
  "icon": "logo.png",
  "engines": {
    "vscode": "^1.80.0"
  }
}
